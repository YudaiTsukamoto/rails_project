<h1>Into Item</h1>
<%= form_for(@item) do |f| %>
  <%= f.label :name %>
  <%= f.text_field:name %>

  <%= f.label :price %>
  <%= f.text_field:price %>

  <%= f.label :content %>
  <%= f.text_field :content %>

  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.submit "New Item" %>
<% end %>
